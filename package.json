{
	"name": "cloud-native-internal",
	"description": "Starter scaffold for Internal Cloud Native Applications",
	"homepage": "https://github.com/sotekton/cloud-native-internal#readme",
	"version": "0.0.0",
	"main": "index.js",
	"keywords": [],
	"dependencies": {
		"abstract-cache": "1.0.1",
		"abstract-cache-redis": "1.0.2",
		"ajv": "6.8.1",
		"authenticator": "1.1.5",
		"await-result": "2.2.1",
		"casbin": "2.0.0",
		"config": "3.0.1",
		"cuid": "2.1.4",
		"dayjs": "1.8.5",
		"deeps": "1.4.5",
		"emittery": "0.4.1",
		"fast-json-patch": "2.0.7",
		"fastify": "1.14.1",
		"fastify-accepts-serializer": "1.0.2",
		"fastify-auth": "0.3.0",
		"fastify-blipp": "1.2.1",
		"fastify-caching": "3.0.0",
		"fastify-redis": "3.0.0",
		"fastify-sensible": "1.0.0",
		"flatted": "2.0.0",
		"got": "9.6.0",
		"hasha": "3.0.0",
		"ioredis": "4.6.2",
		"is-installed": "2.0.1",
		"knex": "0.16.3",
		"knex-migrate": "1.7.0",
		"latest-semver": "1.0.0",
		"is-semver": "1.0.9",
		"make-promises-safe": "4.0.0",
		"map-factory": "3.8.0",
		"muggle-test": "1.0.0",
		"muggle-assert": "1.1.3",
		"obj-watcher": "2.0.15",
		"objection": "1.5.3",
		"objection-db-errors": "1.0.0",
		"objection-soft-delete": "1.0.5",
		"per-env": "1.0.2",
		"permit": "0.2.4",
		"pg": "7.8.0",
		"pino": "5.11.1",
		"pino-debug": "1.2.0",
		"rambdax": "1.7.1",
		"redrun": "7.1.4",
		"resquire": "1.1.1",
		"rfdc": "1.1.2",
		"tap-min": "1.3.0",
		"to-json-schema": "0.2.0",
		"upath": "1.1.0",
		"url-parse": "1.4.4",
		"xxhashjs": "0.2.2"
	},
	"devDependencies": {
		"@commitlint/cli": "7.5.0",
		"@commitlint/config-conventional": "7.5.0",
		"@commitlint/lint": "7.5.0",
		"@commitlint/prompt-cli": "7.5.0",
		"acorn": "6.0.7",
		"babel-runtime": "6.26.0",
		"chuhai": "1.2.0",
		"clear-module": "3.1.0",
		"codecov": "3.1.0",
		"documentary": "1.21.1",
		"eslint": "5.13.0",
		"eslint-config-prettier": "4.0.0",
		"eslint-config-xo": "0.26.0",
		"eslint-plugin-ava": "5.1.1",
		"eslint-plugin-import": "2.16.0",
		"eslint-plugin-no-use-extend-native": "0.4.0",
		"eslint-plugin-node": "8.0.1",
		"eslint-plugin-prettier": "3.0.1",
		"eslint-plugin-promise": "4.0.1",
		"eslint-plugin-unicorn": "7.1.0",
		"faker": "4.1.0",
		"fmtconv": "1.0.7",
		"gron": "4.4.0",
		"husky": "1.3.1",
		"lint-staged": "8.1.3",
		"local-repl": "4.0.0",
		"mismatch": "1.0.3",
		"nock": "10.0.6",
		"nyc": "13.2.0",
		"pino-pretty": "2.5.0",
		"prettier": "1.16.4",
		"rewire": "4.0.1",
		"run-tests": "1.0.4",
		"rxjs": "6.4.0",
		"rxjs-compat": "6.4.0",
		"supertest": "3.4.2",
		"testdouble": "3.10.0",
		"tree-node-cli": "1.2.5",
		"updates": "7.0.0",
		"xo": "0.24.0"
	},
	"optionalDependencies": {
		"fastify-cookie": "2.1.6",
		"fastify-nodemailer": "3.0.0",
		"fastify-server-session": "2.0.0",
		"iron": "5.0.6",
		"nanoid": "2.0.1",
		"password-magic": "1.0.0",
		"pbkdf2": "3.0.17"
	},
	"engines": {
		"node": ">=8"
	},
	"files": [
		"index.js"
	],
	"scripts": {
		"commit": "commit",
		"doc": "doc documentary -o README.md",
		"format": "redrun -p format:*",
		"format:prettier": "prettier --write '**/*.{css,md}'",
		"format:xo": "xo ./**/*.js",
		"start": "./node_modules/.bin/knex-migrate up && node index.js",
		"repl": "local-repl",
		"test": "per-env",
		"posttest": "redrun -p codecov:report codecov:check",
		"test:api": "nyc --cache --per-file --silent run-tests '{,!(node_modules|container)/**/}*.?(api).js' | tap-min",
		"test:development": "nyc --cache --per-file --silent run-tests '{,!(node_modules|container)/**/}*.?(spec|sanity).js' | tap-min",
		"test:sanity": "nyc --cache --per-file --silent run-tests '{,!(node_modules|container)/**/}*.?(sanity).js' | tap-min",
		"test:ci": "nyc --cache --per-file --silent run-tests '{,!(node_modules|container)/**/}*.?(spec|sanity|api).js' | tap-min",
		"posttest:ci": "redrun -p codecov:*",
		"codecov:check": "nyc --per-file check-coverage --statements=90 --lines=90 --functions=90",
		"codecov:generate": "nyc report --reporter=text-lcov > coverage.lcov",
		"codecov:report": "nyc report --reporter=text",
		"codecov:upload": "codecov -t || true",
		"prestart:production": "run-tests '{,!(node_modules|container)/**/}*.?(sanity|api).js' | tap-min"
	},
	"license": "SEE LICENSE IN LICENSE",
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -e $GIT_PARAMS",
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"linters": {
			"*.{js}": "xo",
			"*.{css,md}": [
				"prettier --write",
				"git add"
			],
			"*.{spec,sanity,api}.js": [
				"nyc --per-file --check-coverage --statements=90 --lines=90 --functions=90 run-tests"
			],
			"documentary/**/*.md": [
				"redrun doc",
				"git add README.md"
			],
			"package.json": [
				"./cmd/sync-fathomable",
				"git add fathomable.yaml"
			]
		},
		"ignore": [
			"CHANGELOG.md",
			"microgen/**"
		]
	},
	"nyc": {
		"exclude": [
			"**/*.api.js",
			"**/*.sanity.js",
			"**/*.spec.js",
			"**/node_modules/**",
			"container/**",
			"core/index.js",
			"coverage/**",
			"tap.js"
		],
		"include": [
			"core/**/*.js"
		],
		"watermarks": {
			"lines": [
				90,
				95
			],
			"functions": [
				90,
				95
			],
			"branches": [
				90,
				95
			],
			"statements": [
				90,
				95
			]
		}
	},
	"xo": {
		"globals": [
			"$",
			"_",
			"dbError",
			"config"
		],
		"ignores": [
			"python_modules/"
		],
		"prettier": true,
		"rules": {
			"camelcase": "off",
			"generator-star-spacing": "off",
			"import/no-extraneous-dependencies": "off",
			"import/no-unassigned-import": "off",
			"import/no-unresolved": "off",
			"import/order": "off",
			"operator-linebreak": "off"
		},
		"semicolon": false
	}
}
